{
  "plugins": ["prettier-plugin-svelte", "prettier-plugin-tailwindcss"],
  "trailingComma": "none",
  "printWidth": 120,
  "pluginSearchDirs": false,
  "bracketSameLine": true,
  "bracketSpacing": true,
  "htmlWhitespaceSensitivity": "ignore",
  "overrides": [{ "files": "*.svelte", "options": { "parser": "svelte" } }]
}
